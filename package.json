{
  "name": "@flexis/srcset",
  "version": "1.0.0",
  "description": "Highly customizable lib to generating responsive images.",
  "author": "dangreen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrigenSoftware/gulp-srcset.git"
  },
  "bugs": {
    "url": "https://github.com/TrigenSoftware/gulp-srcset/issues"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "generateDocs": "typedoc ./src --out ./docs --excludeExternals --mode modules",
    "checkDeps": "npm-check -s; exit 0",
    "lint": "tslint -p . -t stylish 'src/**/*.{ts,tsx}'",
    "jest": "NODE_ENV=test jest -c jest.config.json",
    "test": "npm run checkDeps && npm run lint && npm run jest",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run test && npm run build"
  },
  "keywords": [
    "srcset",
    "image",
    "picture",
    "responsive"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^10.5.0",
    "@types/sharp": "^0.17.9",
    "@types/vinyl": "^2.0.2",
    "css-mediaquery": "^0.1.2",
    "imagemin": "^5.3.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-svgo": "^6.0.0",
    "imagemin-webp": "^4.0.0",
    "imagemin-zopfli": "^5.1.0",
    "minimatch": "^3.0.4",
    "sharp": "^0.20.5",
    "through2-concurrent": "^1.1.1",
    "vinyl": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.52",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "@types/jest": "^23.1.3",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "eslint": "^5.0.1",
    "eslint-config-trigen": "^3.2.0",
    "husky": "^1.0.0-rc.10",
    "jest": "^23.3.0",
    "npm-check": "^5.7.1",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^4.0.0-beta.5",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-tslint": "^0.1.34",
    "rollup-plugin-typescript2": "^0.15.1",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-trigen": "^3.0.5",
    "typedoc": "^0.11.1",
    "typescript": "2.9.1"
  },
  "files": [
    "lib"
  ]
}
